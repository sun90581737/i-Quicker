                success: function (da) {
                    if (da.length != 0) {
                        $(da).each(function (index, item) {
                            if (item.DeviceName == "CNC1发那科") {        
                                $("#div1").css({ "background-image": "linear-gradient(" + Colour(item.DeviceRunStatus) + ", #0b3447)" });
                                //23ab33绿   f3961d黄     eb5c5d红   第二层颜色0b3447
                                //$("#div1").css({ "background-image": "linear-gradient(#23ab33, #0b3447)" });
                                $("#a1").text(item.DeviceName);
                                $("#b1").text(item.DeviceRunStatus);
                                $("#imageId1").css("display", "block");
                                $("#imageId1").attr("src", "/Content/img/product/collection/cnc1.png");
                                $("#f1").text(item.TodayOutput);
                                $("#g1").text(item.TodayJiadong * 100 + "%");
                                item.DeviceRunStatus == "运行中" ? loadSpindle("maina", item.SpindleSpeed) : loadSpindle("maina", 0);
                                item.DeviceRunStatus == "运行中" ? loadFeed("mainb", item.FeedSpeed) : loadFeed("mainb", 0);
                                //if (item.SpindleSpeed != null && item.SpindleSpeed > 0) {
                                //    $("#div1").css({ "background-image": "linear-gradient(" + item.DeviceLndicatorLight + ", #0b3447)" });
                                //    $("#b1").text(item.DeviceRunStatus);
                                //}
                                //else {
                                //    $("#b1").text("暂停");
