                        radius: "100%",
                        startAngle: 180,
                        endAngle: 0,
                        axisLine: {
                            show: true,
                            lineStyle: { // 属性lineStyle控制线条样式
                                color: [ //表盘颜色
                                    [0.5, "#DA462C"],//0-50%处的颜色
                                    [0.7, "#FF9618"],//51%-70%处的颜色
