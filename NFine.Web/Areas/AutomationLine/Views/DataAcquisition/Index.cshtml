                                item.DeviceRunStatus == "运行中" ? loadSpindle("maina2", item.SpindleSpeed) : loadSpindle("maina2", 0);
                                item.DeviceRunStatus == "运行中" ? loadFeed("mainb2", item.FeedSpeed) : loadFeed("mainb2", 0);
